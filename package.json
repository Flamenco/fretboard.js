{
	"name": "@moonwave99/fretboard.js",
	"version": "0.1.0",
	"description": "Fretboard diagram generator",
	"homepage": "https://fretboardjs.netlify.app/",
	"main": "dist/fretboard.cjs.js",
	"module": "dist/fretboard.esm.js",
	"browser": "dist/fretboard.umd.js",
	"files": [
		"dist/fretboard.*.js"
	],
	"scripts": {
		"test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' npx ava-ts",
		"coverage": "nyc npm run test",
		"lint": "npx eslint ./src --ext .js,.jsx,.ts,.tsx",
		"start:dev": "rollup -c -w",
		"build:prod": "rollup -c",
		"demo:dev": "webpack-dev-server",
		"demo:prod": "webpack",
		"demo:deploy": "npm run build:prod && npm run demo:prod && netlify deploy --dir ./demo --prod"
	},
	"keywords": [
		"music",
		"guitar",
		"fretboard",
		"svg"
	],
	"author": "Diego Caponera <hello@diegocaponera.com>",
	"license": "ISC",
	"ava": {
		"require": [
			"esm"
		]
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^7.1.3",
		"@rollup/plugin-typescript": "^4.1.1",
		"@types/d3-selection": "^1.4.1",
		"@typescript-eslint/eslint-plugin": "^2.31.0",
		"@typescript-eslint/parser": "^2.31.0",
		"ava": "^3.8.2",
		"ava-ts": "^0.25.2",
		"browser-env": "^3.3.0",
		"eslint": "^7.0.0",
		"esm": "^3.2.25",
		"lodash": "^4.17.15",
		"nyc": "^15.0.1",
		"rollup": "^2.9.0",
		"ts-node": "^8.10.1",
		"tslib": "^1.11.2",
		"typescript": "^3.8.3",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.9",
		"webpack-dev-server": "^3.11.0"
	},
	"dependencies": {
		"@tonaljs/note": "^3.5.3",
		"@tonaljs/scale": "^3.2.3",
		"@tonaljs/tonal": "^3.2.2",
		"d3-selection": "^1.4.1"
	}
}
