<!doctype html>
<html class="has-navbar-fixed-top">

<head>
	<meta charset="utf-8">
	<meta name="viewport"
	  content="width=device-width, initial-scale=1">
	<title><%= htmlWebpackPlugin.options.title %></title>
</head>

<body>
	<%= partials.nav('modes') %>
	<main>
		<section class="container">
			<h1><%= examples.modes.title %></h1>
			<section class="description"><%= texts['examples.modes'] %></section>
			<div class="columns">
				<section class="controls column is-three-quarters">
					<div class="control modes"></div>
				</section>
			</div>
			<div class="columns">
				<figure class="column is-three-quarters" id="fretboard"></figure>
				<form class="column api-actions boxes">
					<div class="field">
						<div class="control"></div>
					</div>
				</form>
			</div>
		</section>
	</main>
	<%= partials.footer() %>
	<%= htmlWebpackPlugin
		.tags
		.bodyTags
		.filter((tag) => tag.attributes.src.startsWith('modes'))
		.join('')
	%>
</body>
</html>
