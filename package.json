{
	"name": "@moonwave99/fretboard.js",
	"version": "0.1.1",
	"description": "Fretboard diagram generator",
	"homepage": "https://moonwave99.github.io/fretboard.js/",
	"main": "dist/fretboard.cjs.js",
	"module": "dist/fretboard.esm.js",
	"browser": "dist/fretboard.umd.js",
	"files": [
		"dist/fretboard.*.js"
	],
	"scripts": {
		"test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' npx ava-ts",
		"coverage": "nyc npm run test",
		"lint": "npx eslint ./src --ext .js,.jsx,.ts,.tsx",
		"start:dev": "rollup -c -w",
		"build:prod": "rollup -c",
		"site:dev": "webpack-dev-server",
		"site:prod": "webpack",
		"site:deploy": "npm run build:prod && npm run site:prod && gh-pages -d ./_site"
	},
	"keywords": [
		"music",
		"guitar",
		"fretboard",
		"svg"
	],
	"author": "Diego Caponera <hello@diegocaponera.com>",
	"license": "ISC",
	"ava": {
		"require": [
			"esm"
		]
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^7.1.3",
		"@rollup/plugin-typescript": "^4.1.1",
		"@tonaljs/chord": "^3.6.5",
		"@types/d3-selection": "^1.4.1",
		"@typescript-eslint/eslint-plugin": "^2.31.0",
		"@typescript-eslint/parser": "^2.31.0",
		"abcjs": "^5.12.0",
		"ava": "^3.8.2",
		"ava-ts": "^0.25.2",
		"browser-env": "^3.3.0",
		"bulma": "^0.8.2",
		"copy-webpack-plugin": "^6.0.1",
		"css-loader": "^3.5.3",
		"eslint": "^7.0.0",
		"esm": "^3.2.25",
		"fs-extra": "^9.0.0",
		"gh-pages": "^2.2.0",
		"html-webpack-plugin": "^4.3.0",
		"lodash": "^4.17.15",
		"marked": "^1.1.0",
		"node-sass": "^4.14.1",
		"nyc": "^15.0.1",
		"prismjs": "^1.20.0",
		"rollup": "^2.9.0",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.2.1",
		"ts-node": "^8.10.1",
		"tslib": "^1.11.2",
		"typescript": "^3.8.3",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.9",
		"webpack-dev-server": "^3.11.0"
	},
	"dependencies": {
		"@tonaljs/note": "^3.5.3",
		"@tonaljs/scale": "^3.2.3",
		"@tonaljs/tonal": "^3.2.2",
		"d3-selection": "^1.4.1"
	}
}
