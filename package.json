{
	"name": "fretboard.js",
	"version": "0.0.1",
	"description": "Fretboard diagram generator",
	"main": "dist/fretboard.cjs.js",
	"module": "dist/fretboard.esm.js",
	"browser": "dist/fretboard.umd.js",
	"scripts": {
		"test": "ava",
		"lint": "semistandard ./src/*.js",
		"fix": "semistandard --fix ./src/*.js",
		"start:dev": "rollup -c -w",
		"build:prod": "rollup -c",
		"demo:dev": "webpack-dev-server",
		"demo:prod": "webpack"
	},
	"keywords": [
		"music",
		"guitar",
		"fretboard",
		"svg"
	],
	"author": "Diego Caponera <hello@diegocaponera.com>",
	"license": "ISC",
	"ava": {
		"files": [
			"**/*.test.js"
		],
		"require": [
			"esm"
		]
	},
	"devDependencies": {
		"ava": "^2.4.0",
		"esm": "^3.2.25",
		"rollup": "^1.26.3",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"semistandard": "^14.2.0",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.9",
		"webpack-dev-server": "^3.9.0"
	},
	"dependencies": {
		"@tonaljs/scale": "^3.2.3",
		"@tonaljs/tonal": "^3.2.2",
		"d3-selection": "^1.4.0",
		"decamelize": "^3.2.0"
	}
}
