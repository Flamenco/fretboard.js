<!doctype html>
<html class="has-navbar-fixed-top">

<head>
	<meta charset="utf-8">
	<meta name="viewport"
	  content="width=device-width, initial-scale=1">
	<title><%= htmlWebpackPlugin.options.title %></title>
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.8.2/css/bulma.min.css">
</head>

<body>
	<%= partials.nav('playback') %>
	<main class="container">
		<h1><%= examples.playback.title %></h1>
		<section class="columns">
			<div class="column is-three-quarters">
				<figure id="notation"></figure>
				<div id="audio"></div>
				<figure id="fretboard"></figure>
			</div>
			<aside class="column aside"><%= texts['examples.playback'] %></aside>
		</section>
	</main>
	<%= partials.footer() %>
	<%= htmlWebpackPlugin
	  .tags
	  .bodyTags
	  .filter((tag) => tag.attributes.src.startsWith('playback'))
	  .join('')
	%>
</body>
</html>
